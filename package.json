{
  "name": "tailwind-storybook",
  "version": "0.1.0",
  "private": true,
  "author": "Mondra",
  "dependencies": {
    "@mondra/ui-components": "^2.2.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.38",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "classnames": "^2.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "swr": "^1.3.0",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --config .prettierrc.js --write src/",
    "lint": "eslint -c .eslintrc.js --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint -c .eslintrc.js --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx --fix src/",
    "formatter": "npm run lint:fix && npm run prettier",
    "package-report:dev": "license-report --output=json --config license-report-config.json --only=dev",
    "package-report:main": "license-report --output=json --config license-report-config.json --only=prod",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mdx-js/react": "^1.6.22",
    "@mondra/code-format-config": "^1.0.5",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^6.5.9",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/forms": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "babel-plugin-named-exports-order": "^0.0.2",
    "husky": "^8.0.0",
    "license-report": "^5.0.2",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.73.0"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}
